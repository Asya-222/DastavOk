<script>

    var settings = {"provider":"osm"};

	var map = new L.Map('AzurMap',{
	scrollWheelZoom:false, 
	center: new L.LatLng(43.42455, 39.91920), 
	maxZoom: 20,
	zoom: 16, 
	zoomControl: false });
	
	var layers = {};
    var primary;

    var osm = new L.TileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png');
    var yndx = new L.Yandex();
    //var yndx = new L.TileLayer('http://vec02.maps.yandex.net/tiles?l=map&v=2.16.0&x={x}&y={y}&z={z}', {maxZoom:20});
    var ggl2 = new L.Google('HYBRID', {detectRetina: true});
    var ggl = new L.TileLayer('http://{s}.google.com/vt/lyrs=m&x={x}&y={y}&z={z}',  {
        maxZoom: 20,
        subdomains:['mt0','mt1','mt2','mt3']
    });

    if(settings.provider){
        switch (settings.provider){
            case 'osm':
                primary = osm;
                layers.OSM = osm;
                layers.Yandex = yndx;
                layers.Google = ggl;
            break;

            case 'yandex':
                primary = yndx;
                layers.Yandex = yndx;
                layers.OSM = osm;
                layers.Google = ggl;
            break;

            case 'google':
                primary = ggl;
                layers.Google = ggl;
                layers.OSM = osm;
                layers.Yandex = yndx;
            break;
        }
    }else{
        primary = osm;
        layers.OSM = osm;
        layers.Yandex = yndx;
        layers.Google = ggl;
    }
    //
    
    //layers.GoogleHybrid = ggl2;
    
    layers.GoogleHybrid = ggl2;

    map.addLayer(primary);

    map.addControl(new L.Control.Layers(layers));
    
	//map.addControl(new L.Control.Layers({'Карта':azurmap, 'Спутник Bing':bing}, {}));
var zoomFS = new L.Control.ZoomFS();
map.addControl(zoomFS);

	var marker = new L.Marker(new L.LatLng(43.42486, 39.91831));//, {icon: L.icon({iconUrl: '/img/leaflet/images/marker.png', iconSize: [25, 41], iconAnchor: [13, 41], popupAnchor: [0, -30]})});
	map.addLayer(marker);
	marker.bindPopup('Ресторан &quot;Атлант&quot;').openPopup();
	var d = new L.Control.Distance({}); map.addControl(d);
	map.addControl(new L.Control.Scale());
	
	var d_metrs = document.getElementById('distance_metrs') || {};
	var d_cords = document.getElementById('distance_cords') || {};
	if( d_cords.value ) addDistanceCords( d_cords.value, d_metrs.value );
	function addDistanceCords(cords, metrs) {
		if( cords ) {
			metrs = metrs || 0;
			var dc = new L.Polyline([], {editable: false});
			
			lls = cords.split(';');
		
			for (var i = 0; i < lls.length; i++) {
				if( lls[i] ) ll = lls[i].split(',');
				dc.addLatLng( new L.latLng(ll[1], ll[0]) );
			}
			dc.bindPopup('Растояние до моря '+ metrs +' м');
			map.addControl(dc);
		}
	}
	
</script>
